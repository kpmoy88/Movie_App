<%= form_for @movie do |f| %>
  <fieldset>
    <div class="fields">
      <%= f.label :title %>
      <%= f.text_field :title %>
    </div>
    <div class="fields">
      <%= f.label :description %>
      <%= f.text_field :description %>
    </div>
    <div class="fields">
      <%= f.label :year_released, "Year Released" %>
      <%= f.text_field :year_released %>
    </div>
    <div class="fields">
      <%= f.label :rating %>
      <%= f.text_field :rating %>
    </div>
  </fieldset>
  <%= f.submit %>
<% end %>
